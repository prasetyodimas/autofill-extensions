{
  "manifest_version": 3,
  "name": "Autofill Extension",
  "version": "1.0",
  "description": "Autofill form fields from a saving your time when logged in to a website.",
  "permissions": ["scripting", "activeTab"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}
